{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "api",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "node_modules/wrangler/bin/wrangler.js",
        "dev",
        "--cwd",
        "apps/api"
      ],
      "port": 8787
    },
    {
      "name": "mobile",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "-e",
        "process.chdir('apps/mobile'); require('child_process').execFileSync(process.execPath, ['../../node_modules/@expo/cli/build/bin/cli', 'start', '--web', '--port', '8081'], {stdio: 'inherit'})"
      ],
      "port": 8081
    }
  ]
}
