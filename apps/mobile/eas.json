{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "prebuildCommand": "cd ../.. && pnpm exec nx run-many -t build -p @nx-monorepo/api-client @nx-monorepo/schemas"
    },
    "production": {
      "extends": "base",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://nx-monoreposerver-production.up.railway.app/api"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://nx-monoreposerver-staging.up.railway.app/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
