{
  "name": "@nx-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "22.x",
    "npm": "please-use-pnpm",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "nx run-many -t dev --projects=web,server --parallel=2",
    "dev:web": "nx run web:dev",
    "dev:server": "nx run server:serve",
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "test:coverage": "nx run-many -t test --coverage",
    "test:coverage:web": "nx run web:test --coverage",
    "test:coverage:server": "nx run server:test --coverage",
    "lint": "nx run-many -t lint",
    "e2e": "nx run-many -t e2e",
    "format:check": "nx run-many -t format:check",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,html}\"",
    "validate:research": "node tools/scripts/validate-research.js",
    "validate:governance": "node tools/gates/run-internal-alignment.mjs",
    "validate:env": "node scripts/validate-env.js",
    "prepare": "husky",
    "db:push:dev": "dotenv -e .env.development.local -- npx prisma db push --schema=packages/database/prisma/schema.prisma",
    "db:push:test": "dotenv -e .env.test.local -- npx prisma db push --schema=packages/database/prisma/schema.prisma",
    "db:migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev --schema=packages/database/prisma/schema.prisma",
    "db:migrate:test": "dotenv -e .env.test.local -- npx prisma migrate dev --schema=packages/database/prisma/schema.prisma",
    "db:migrate:deploy:dev": "dotenv -e .env.development.local -- npx prisma migrate deploy --schema=packages/database/prisma/schema.prisma",
    "db:migrate:deploy:test": "dotenv -e .env.test.local -- npx prisma migrate deploy --schema=packages/database/prisma/schema.prisma",
    "db:studio:dev": "dotenv -e .env.development.local -- npx prisma studio --schema=packages/database/prisma/schema.prisma",
    "db:studio:test": "dotenv -e .env.test.local -- npx prisma studio --schema=packages/database/prisma/schema.prisma",
    "db:generate": "dotenv -e .env.development.local -- npx prisma generate --schema=packages/database/prisma/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "next": "16.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "prisma": "^6.17.1",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "^9.8.0",
    "@next/eslint-plugin-next": "16.0.8",
    "@nx/devkit": "22.2.0",
    "@nx/esbuild": "22.2.0",
    "@nx/eslint": "22.2.0",
    "@nx/eslint-plugin": "22.2.0",
    "@nx/jest": "22.2.0",
    "@nx/js": "22.2.0",
    "@nx/next": "22.2.0",
    "@nx/node": "22.2.0",
    "@nx/playwright": "22.2.0",
    "@nx/react": "22.2.0",
    "@nx/workspace": "22.2.0",
    "@playwright/test": "1.56.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "10.4.13",
    "babel-jest": "30.2.0",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.19.2",
    "eslint": "9.37.0",
    "eslint-config-next": "16.0.8",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-environment-node": "30.2.0",
    "jest-util": "30.2.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^16.2.5",
    "nx": "22.2.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "packageManager": "pnpm@10.19.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "esbuild",
      "sharp"
    ],
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0"
    }
  }
}
